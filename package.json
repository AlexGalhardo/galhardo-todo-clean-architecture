{
	"name": "todo-api-clean-architecture",
	"engines": {
		"node": ">=20"
	},
	"prisma": {
		"seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
	},
	"scripts": {
		"start": "npm run build && node dist/src/server.js",
		"nodemon": "npx nodemon dist/server.js",
		"pm2": "npx pm2 start dist/server.js --name galhardo-app-api --interpreter node --watch",
		"dev": "concurrently \"npm run delete:build\" \"npm run watch:compile\" \"node ./dist/server.js\"",
		"watch:compile": "npm run delete:build && swc src -w --out-dir dist",
		"watch:dev": "npx nodemon ./dist/server.js",
		"build": "npm run delete:build && swc src -d dist",
		"test": "vitest",
		"vitest:coverage": "vitest run --coverage",
		"format": "prettier --write \"src/**/*.ts\"",
		"delete:build": "rm -rf dist",
		"postinstall": "npx prisma generate",
		"prepare": "npx husky install"
	},
	"dependencies": {
		"@faker-js/faker": "^8.4.0",
		"@prisma/client": "^5.8.1",
		"bcrypt": "^5.1.1",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.4.1",
		"express": "^4.18.2",
		"express-async-errors": "^3.1.1",
		"helmet": "^7.1.0",
		"jsonwebtoken": "^9.0.2",
		"rand-token": "^1.0.1",
		"husky": "^8.0.3"
	},
	"devDependencies": {
		"@swc/cli": "^0.3.5",
		"@swc/core": "^1.3.107",
		"@trunkio/launcher": "^1.3.0",
		"@types/bcryptjs": "^2.4.6",
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/node": "^20.11.10",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.20.0",
		"@typescript-eslint/parser": "^6.20.0",
		"@vitest/coverage-c8": "^0.28.5",
		"c8": "^9.1.0",
		"concurrently": "^8.2.2",
		"eslint": "^8.56.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-import-helpers": "^1.3.1",
		"eslint-plugin-prettier": "^5.1.3",
		"install": "^0.13.0",
		"nodemon": "^3.0.3",
		"npm": "^10.4.0",
		"prettier": "^3.2.4",
		"prisma": "^5.8.1",
		"superagent": "^8.1.2",
		"supertest": "^6.3.4",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "^5.3.3",
		"vitest": "^1.2.2",
		"@types/bcrypt": "^5.0.2"
	}
}